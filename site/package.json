{
  "name": "next-commerce",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused"
  },
  "sideEffects": false,
  "dependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/runtime": "^7.20.1",
    "@radix-ui/react-dropdown-menu": "^1.0.0",
    "@react-spring/web": "^9.5.5",
    "@solana/wallet-adapter-base": "^0.9.18",
    "@solana/wallet-adapter-react": "^0.15.24",
    "@solana/wallet-adapter-react-ui": "^0.9.22",
    "@solana/wallet-adapter-wallets": "^0.19.5",
    "@solana/web3.js": "^1.66.2",
    "@vercel/commerce": "workspace:0.0.1",
    "@vercel/commerce-bigcommerce": "workspace:0.0.1",
    "@vercel/commerce-commercejs": "workspace:0.0.1",
    "@vercel/commerce-kibocommerce": "workspace:0.0.1",
    "@vercel/commerce-local": "workspace:0.0.1",
    "@vercel/commerce-ordercloud": "workspace:0.0.1",
    "@vercel/commerce-saleor": "workspace:0.0.1",
    "@vercel/commerce-sfcc": "workspace:0.0.1",
    "@vercel/commerce-shopify": "workspace:0.0.1",
    "@vercel/commerce-spree": "workspace:0.0.1",
    "@vercel/commerce-swell": "workspace:0.0.1",
    "@vercel/commerce-vendure": "workspace:0.0.1",
    "autoprefixer": "^10.4.13",
    "body-scroll-lock": "^4.0.0-beta.0",
    "bs58": "^4.0.1",
    "clsx": "^1.2.1",
    "email-validator": "^2.0.4",
    "js-cookie": "^3.0.1",
    "keen-slider": "^6.8.3",
    "lodash.random": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "next": "^12.3.2",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.18",
    "postcss-nesting": "^10.2.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-fast-marquee": "^1.3.5",
    "react-merge-refs": "^2.0.1",
    "react-native": "^0.70.4",
    "react-use-measure": "^2.1.1",
    "tabbable": "^5.3.3",
    "tailwindcss": "^3.2.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.3.2",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash.random": "^3.2.7",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.26.0",
    "eslint-config-next": "^12.3.2",
    "eslint-config-prettier": "^8.5.0",
    "lint-staged": "^13.0.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
